<application>
  <component name="EditorSettings">
    <option name="USE_SOFT_WRAPS" value="MAIN_EDITOR:CONSOLE" />
    <option name="IS_VIRTUAL_SPACE" value="true" />
    <option name="IS_WHEEL_FONTCHANGE_ENABLED" value="true" />
  </component>
  <component name="SqlEditorOptions">
    <option name="concatenateStringsOnEnter" value="true" />
    <option name="qualificationType" value="On collisions" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
    <pattern pattern="qa" />
    <pattern useCustomColors="true" case-sensitive="true" pattern="CR:">
      <option name="FOREGROUND" value="9211ff" />
      <option name="FONT_TYPE" value="3" />
      <option name="ERROR_STRIPE_COLOR" value="cd15ff" />
    </pattern>
    <filter name="CR">
      <pattern index="2" />
    </filter>
  </component>
  <component name="XmlEditorOptions">
    <option name="insertQuotesForAttributeValue" value="false" />
  </component>
</application>